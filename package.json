{
  "name": "fixtergeek-mcp-server",
  "version": "0.0.7",
  "description": "Servidor MCP (Model Context Protocol) modular y extensible con integraciÃ³n de LLMs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.d.ts",
      "require": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test:run",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "git:push": "./scripts/git-push.sh",
    "publish:patch": "./scripts/npm-publish.sh",
    "publish:minor": "npm version minor && ./scripts/npm-publish.sh",
    "publish:major": "npm version major && ./scripts/npm-publish.sh",
    "publish:custom": "./scripts/npm-publish.sh"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "llm",
    "ai",
    "server",
    "http",
    "websocket",
    "openai",
    "ollama",
    "anthropic",
    "typescript",
    "nodejs",
    "api"
  ],
  "author": "FixterGeek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blissito/MCP_Typescript_SDK_Server.git"
  },
  "bugs": {
    "url": "https://github.com/blissito/MCP_Typescript_SDK_Server/issues"
  },
  "homepage": "https://github.com/blissito/MCP_Typescript_SDK_Server#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
